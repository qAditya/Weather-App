<body>

    {{>navbar}} {{!-- handlebar expression for rendering navbar partial--}}
    <div class="container-fluid main_header">
       <div class="row">
           <div class="col-md-10 col-12 mx-auto">
               <div class="row">
           <div class="col-md-6 col-12  main_header_left">
            <h3 id="welcome">Welcome to the weather app</h3>
            <h1>Get the latest <span>Weather</span> update about your city</h1>
            <p>Want to plan to go outside,but not sure of the weather conditions.To know the latest weather conditions</p>
            <button class="btn btn-primary"><a href="/weather"> Click Here</a>  </button>
            </div>
            <div class="col-md-6 col-12  main_header_right mt-5">
                <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                    <ol class="carousel-indicators">
                      <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                      <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                      <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                    </ol>
                    <div class="carousel-inner">
                        <div class="carousel-item active">
                            <img class="d-block " src="data:image/png;base64," alt="First slide">
                          </div>
                      <div class="carousel-item ">
                        <img class="d-block " src="https://solarsystem.nasa.gov/system/basic_html_elements/11561_Sun.png" alt="Second slide">
                      </div>
                     
                      <div class="carousel-item">
                        <img class="d-block " src="http://st2.depositphotos.com/4155807/6036/v/600/depositphotos_60363277-stock-illustration-girl-with-umbrella.jpg" alt="Third slide">
                      </div>
                    </div>
                    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                      <span class="sr-only">Previous</span>
                    </a>
                    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                      <span class="carousel-control-next-icon" aria-hidden="true"></span>
                      <span class="sr-only">Next</span>
                    </a>
                  </div>
            </div>
        </div>
        </div>

        </div>
    </div>
    
